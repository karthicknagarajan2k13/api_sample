<div class="row">
  <div class="col-md-4">
  </div>
  <div class="col-md-4">
    <%= render partial: 'shared/error_messages', locals: {object: @admin_post} %>

    <div class="title-head">Listing Admin Posts</div>
    <table class="table">
      <thead>
        <tr>
         <!--  <th>Url</th> -->
         <th>Title</th>
         <th>Message</th>
         <th>Is&nbsp;active</th>
         <th colspan="3"></th>
        </tr>
      </thead>

     <tbody class="prophent-delete ">
      <% @admin_posts.each do |admin_post| %>
      <tr>
        <!-- <td><%= admin_post.url %></td> -->
        <td><%= admin_post.title %></td>
        <td><%= admin_post.message %></td>
        <td><%= admin_post.is_active %></td>
        <td><%= link_to 'Show', admin_post %></td>
        <td><%= link_to 'Edit', edit_admin_post_path(admin_post) %></td>
        <td><%= link_to 'Destroy', admin_post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
      <% end %>
    </tbody>
    </table>
    <%= will_paginate @admin_posts, renderer: BootstrapPagination::Rails %>
    <br>
    <%= link_to 'New Admin post', new_admin_post_path, :class => "btn-prophent btn-primary btn-lg btn-block" %>
  </div>
  <div class="col-md-4">
  </div>
</div>
