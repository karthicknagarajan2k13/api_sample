<div class="side-bar hidden" style="cursor: pointer;"  onclick = "void(0)"></div>
<div class="side-bar-menu">
  <% if user_signed_in? %>
    <ul>
      <div class="sidebar-profile-logo">
        <% if current_user.image? %>
         <li class="cur-user-img" style=" box-shadow: none !important;"><%= image_tag current_user.image_url, class:'sidebar-profile-img' %></li>
        <% else %>
          <li class="cur-user-img" style=" box-shadow: none !important;"><img src="/assets/no-img.jpg" class="sidebar-profile-img"></li>
        <% end %>
        <li class="cur-user-img text-center" style=" box-shadow: none !important; color:#000;margin-top:0px !important"><span><%= current_user.try(:name)%></span></li>
      </div>
      <% if current_user and current_user.admin? %>
         <li class="home_menu "><%= link_to "Prophet", homes_admin_path%> </li>
         <li class="home_menu"> <%= link_to "Request Messages", prophent_messages_path%> </li>
         <li class="home_menu"><%= link_to "User Request", prophents_path %> </li>
         <li class="home_menu"><%= link_to "Info", admin_posts_path%> </li>
         <li class="home_menu"> <%= link_to "Youtube Video Url", video_url_path%></li>
      <%end%>
         <li class="home_menu"><%= link_to 'Edit profile', edit_user_registration_path%> </li>
         <li class="home_menu"><%= link_to "Logout", destroy_user_session_path, method: :delete%></li>
      <%else%>
        <%#= link_to "Sign up", new_user_registration_path, :class => 'navbar-link'  %> 
        <%#= link_to "Login", new_user_session_path, :class => 'navbar-link'  %>
      <%end%>

      </ul>
  </div>
</div>


